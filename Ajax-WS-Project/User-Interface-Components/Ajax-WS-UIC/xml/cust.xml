<?xml version="1.0" encoding="UTF-8"?>
<natpage translationreference="bmobile" natsource="CUST" natsinglebyte="true" natkcheck="true" xmlns:njx="http://www.softwareag.com/njx/njxMapConverter" xmlns:bmobile="http://www.softwareag.com/njx/njxMobile">
    <autocomplete id="CustomerIDvalues" source="string" sourcelocation="605;606;607">
    </autocomplete>
    <bmobile:container takefullwidth="true">
        <bmobile:panel>
            <bmobile:panelheader name="Registration" textid="RegistrationTextID">
            </bmobile:panelheader>
            <bmobile:panelbody>
                <bmobile:row>
                    <bmobile:col styleclasses="col-lg-2 col-md-2">
                        <bmobile:flabel name="Customer ID" textid="CustomerIDTextID">
                        </bmobile:flabel>
                    </bmobile:col>
                    <bmobile:col styleclasses="col-lg-4 col-md-4">
                        <bmobile:ffield autocompleteref="CustomerIDvalues" autocompletewithdropdown="true" placeholder="customer identification" valueprop="myCustID">
                        </bmobile:ffield>
                    </bmobile:col>
                    <bmobile:col styleclasses="col-lg-4 col-md-4">
                        <bmobile:flabel name="Password">
                        </bmobile:flabel>
                    </bmobile:col>
                    <bmobile:col styleclasses="col-lg-2 col-md-2">
                        <bmobile:ffield placeholder="Password" valueprop="myPassword">
                        </bmobile:ffield>
                    </bmobile:col>
                </bmobile:row>
                <bmobile:row>
                    <bmobile:col styleclasses="col">
                        <bmobile:button name="OK" styleclasses="btn-default" method="onRegistration">
                        </bmobile:button>
                    </bmobile:col>
                </bmobile:row>
            </bmobile:panelbody>
        </bmobile:panel>
        <bmobile:panel>
            <bmobile:panelheader name="Customer data">
            </bmobile:panelheader>
            <bmobile:panelbody>
                <bmobile:row>
                    <bmobile:col styleclasses="col">
                        <bmobile:formarea>
                            <bmobile:formgroup>
                                <bmobile:flabel styleclasses="col-lg-1 col-md-1" name="Lastname">
                                </bmobile:flabel>
                                <bmobile:col styleclasses="col-lg-5 col-md-5">
                                    <bmobile:ffield placeholder="MyLastnameIsVeryLong" valueprop="myLastname">
                                    </bmobile:ffield>
                                </bmobile:col>
                                <bmobile:flabel styleclasses="col-lg-2 col-md-2" name="Firstname">
                                </bmobile:flabel>
                                <bmobile:col styleclasses="col-lg-4 col-md-4">
                                    <bmobile:ffield placeholder="My Firstname" valueprop="myFirstname">
                                    </bmobile:ffield>
                                </bmobile:col>
                            </bmobile:formgroup>
                            <bmobile:formgroup>
                                <bmobile:flabel styleclasses="col-lg-1 col-md-1" name="City">
                                </bmobile:flabel>
                                <bmobile:col styleclasses="col-lg-2 col-md-3">
                                    <bmobile:ffield placeholder="My City Home" valueprop="myCity">
                                    </bmobile:ffield>
                                </bmobile:col>
                                <bmobile:flabel styleclasses="col-lg-1 col-md-1" name="ZIP">
                                </bmobile:flabel>
                                <bmobile:col styleclasses="col-lg-2 col-md-2">
                                    <bmobile:ffield placeholder="12345" valueprop="myZIP">
                                    </bmobile:ffield>
                                </bmobile:col>
                                <bmobile:flabel styleclasses="col-lg-2 col-md-1" name="Street">
                                </bmobile:flabel>
                                <bmobile:col styleclasses="col-lg-4 col-md-4">
                                    <bmobile:ffield placeholder="my Street" valueprop="myStreet">
                                    </bmobile:ffield>
                                </bmobile:col>
                            </bmobile:formgroup>
                        </bmobile:formarea>
                    </bmobile:col>
                </bmobile:row>
            </bmobile:panelbody>
        </bmobile:panel>
    </bmobile:container>
    <bmobile:dynmodal>
    </bmobile:dynmodal>
</natpage>
