<?xml version="1.0" encoding="UTF-8"?>
<natpage natsource="SIDE"  uselatestbootstrap="true" natsinglebyte="true" natkcheck="true" xmlns:njx="http://www.softwareag.com/njx/njxMapConverter" xmlns:bmobile="http://www.softwareag.com/njx/njxMobile">
    <njx:eventdata>
    </njx:eventdata>
    <bmobile:sidebarcontainer>
        <bmobile:sidebar toggleid="MYSIDEBAR1">
            <bmobile:sidebargroup>
                <bmobile:bartext name="Sidebar Header" styleclasses="navbar-brand">
                </bmobile:bartext>
            </bmobile:sidebargroup>
            <bmobile:sidebargroup>
                <bmobile:form>
                    <bmobile:formgroup>
                        <bmobile:flabel name="User">
                        </bmobile:flabel>
                        <bmobile:ffield styleclasses="form-control-sm" valueprop="user">
                        </bmobile:ffield>
                    </bmobile:formgroup>
                    <bmobile:button name="Login" styleclasses="btn-block btn-info btn-sm" method="onLogin">
                    </bmobile:button>
                </bmobile:form>
            </bmobile:sidebargroup>
            <bmobile:navmenu valueprop="mymenu">
            </bmobile:navmenu>
        </bmobile:sidebar>
        <bmobile:content>
            <bmobile:horizontalbar>
                <bmobile:toggler title="Toggle sidebar" toggleref="MYSIDEBAR1">
                </bmobile:toggler>
                <bmobile:horizontalgroup styleclasses="ml-3 mr-auto">
                    <bmobile:bartext name="Application Header " styleclasses="navbar-brand">
                    </bmobile:bartext>
                </bmobile:horizontalgroup>
                <bmobile:forminline styleclasses="my-auto">
                    <bmobile:ffield styleclasses="form-control-sm d-none d-md-inline-block" valueprop="mysearch">
                    </bmobile:ffield>
                    <bmobile:button name="Search" styleclasses="btn-outline-info btn-sm mr-3 d-none d-md-inline-block" method="onSearch">
                    </bmobile:button>
                    <bmobile:button name="&lt;b&gt;&amp;times;&lt;/b&gt;" styleclasses="close mr-0 my-0" type="button" method="onExit" title="Close Application">
                    </bmobile:button>
                </bmobile:forminline>
            </bmobile:horizontalbar>
            <bmobile:container takefullwidth="true" takefullheight="true">
                <bmobile:pageheader name="Page Content">
                </bmobile:pageheader>
            </bmobile:container>
        </bmobile:content>
    </bmobile:sidebarcontainer>
    <bmobile:dynmodal>
    </bmobile:dynmodal>
</natpage>

